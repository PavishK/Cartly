{
  "cli": {
    "version": ">=16.19.2",
    "appVersionSource": "remote"
  },
  "build": {
    "apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_SERVER_URL":"@EXPO_PUBLIC_SERVER_URL"
      }
    }
  },
  "submit": {
    "production": {}
  }
}